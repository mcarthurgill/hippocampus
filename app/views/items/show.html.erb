
<div class="row">
  <div class="col-xs-12">
    <ol class="breadcrumb">
      <li><a href="<%= user_path(current_user) %>">Notes</a></li>
      <li class="active">Note</li>
    </ol>
  </div>
</div>

<div class="row">
  <div class="col-xs-12">

    <% if notice %>
      <div class="row">
        <div class="col-xs-12">
          <br/>
          <div class="alert alert-info alert-dismissible" role="alert">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <%= notice %>
          </div>
        </div>
      </div>
    <% end %>
    
    <%= render 'detail' %>

    <%= render 'buckets' %>

    <%= render 'footer' %>

  </div>
</div>

<br/>
<br/>
