<%= form_for @item, html: {:multipart => true} do |f| %>
  <!-- Main Menu -->
  <%= render "shared/menus/desktop/item-main-menu", :item => @item, :f => f %>

  <!-- Side Menu -->
  <%= render "shared/menus/desktop/item-side-menu", :item => @item, :f => f %>

  <!-- Main Content -->
  <%= render 'shared/items/main_content', :item => @item, :f => f %>
  
<% end %>